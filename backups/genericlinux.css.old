@font-face {
  font-family: ubuntu;
  src: url("/Ubuntu-R.ttf");
}
#navbar {
	background-color: #405;
	color: white;
	width: 100%;
	margin: 0px;
	padding: 10px;
	padding-top: 15px;
}
a {
	color: magenta;
}
hr {
	border-color: purple;
}
a:hover {
	color: white;
}
a:active {
	color: grey;
}
.clickable {
	cursor: pointer;
}
.clickable:active {
	color: magenta;
}
.jumbotron {
	color: white;
	background-color: purple;
	background: url('/banner.png');
	background-repeat: no-repeat;
    background-size: zoom;
	padding: 20px;
	padding-left: 15%;
	padding-right: 15%;
	width: 100%;
}
.disclaimer {
	color: white;
	background-color: purple;
	padding: 10px;
	padding-bottom: 1px;
	padding-top: 15px;
}
.disclaimer a {
	color: white;
}
.disclaimer a:hover {
	color: white;
}
.disclaimer a:active {
	color: white;
}
.content {
	padding: 20px;
	padding-left: 15%;
	padding-right: 15%;
	width: 100hh;
}
.alert {
	background-color: #e00;
	color: white;
	padding: 7.5px;
	padding-top: 10px;
	border-radius: 6px;
}
body {
	color: white;
	background-color: black;
	margin-top: 0px;
	margin: 0px;
	font-family: ubuntu;
}
h1 {
	margin-bottom: 0px;
}
